<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.onore.project.member.mapper.MemberMapper">
	<select id="getMemberInfo" resultType="com.onore.project.member.dto.MembersDTO">
		SELECT * FROM members WHERE mem_id = #{mem_id}
	</select>
	<select id="getCoupons" resultType="com.onore.project.member.dto.CouponDTO">
		SELECT * FROM coupon WHERE mem_id = #{mem_id}
	</select>
</mapper>